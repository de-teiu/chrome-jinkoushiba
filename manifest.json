{
    "manifest_version": 2,
    "name": "人工芝",
    "description": "githubのユーザーページに草をたくさん生やしているように見せかけます.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
				"https://github.com/*"
			],
            "js": [
				"vendor/jquery-2.2.4.js",
				"jinkoushiba.js"
			],
            "run_at": "document_start"
		}
	],
    "web_accessible_resources": [
	]
}
