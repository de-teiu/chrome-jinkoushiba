{
    "manifest_version": 2,
    "name": "人工芝",
    "short_name": "人工芝",
    "description": "GitHubのユーザーページのContributionを草まみれにする拡張機能です.",
    "version": "1.0",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://github.com/*"],
            "js": ["jinkoushiba.js"],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": []
}
